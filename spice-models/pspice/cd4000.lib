+   TOD2 = { ( (IN_EN & D2) | (IN_DIS & Q2) ) }
+   TOD3 = { ( (IN_EN & D3) | (IN_DIS & Q3) ) }
+   TOD4 = { ( (IN_EN & D4) | (IN_DIS & Q4) ) }
