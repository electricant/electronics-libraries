+   LOADBAR = { LOADBAR_I }
+   P1 = { P1_I }
+   P2 = { P2_I }
+   P3 = { P3_I }
+   P4 = { P4_I }
+
+   EN = { ~(PE & TE) }
+   LOAD = { ~LOADBAR }
+   TOD1 = { (P1 & LOAD) | (LOADBAR & ~(Q1BAR ^ ~EN)) }
+   TOD2 = { (P2 & LOAD) | (LOADBAR & ~(Q2BAR ^ ~(Q1BAR | EN | Q4))) }
+   TOD3 = { (P3 & LOAD) | (LOADBAR & ~(Q3BAR ^ ~(Q1BAR | EN | Q2BAR))) }
+   TOD4 = { (P4 & LOAD) | (LOADBAR & ~(Q4BAR ^
+     ~((Q1BAR | Q2BAR | Q3BAR | EN) & (Q1BAR | EN | Q4BAR)))) }
+   CARRYOUT = { Q1 & Q4  & TE }
