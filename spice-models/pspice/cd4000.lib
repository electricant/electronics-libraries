+ LOGIC:
+   WDISABLE = { WDISABLE_I }
+   RESET    = { RESET_I }
+   DATA     = { DATA_I }
+   A0       = { A0_I }
+   A1       = { A1_I }
+   A2       = { A2_I }
+   A0BAR    = { ~A0 }
+   A1BAR    = { ~A1 }
+   A2BAR    = { ~A2 }
+
+   RBAR     = { ~RESET }
+   WRABLE   = { ~WDISABLE }
+   ADDR0    = { A2BAR & A1BAR & A0BAR }
+   ADDR1    = { A2BAR & A1BAR & A0    }
+   ADDR2    = { A2BAR & A1    & A0BAR }
+   ADDR3    = { A2BAR & A1    & A0    }
+   ADDR4    = { A2    & A1BAR & A0BAR }
+   ADDR5    = { A2    & A1BAR & A0    }
+   ADDR6    = { A2    & A1    & A0BAR }
+   ADDR7    = { A2    & A1    & A0    }
+   GATE0    = { WRABLE & ADDR0 }
+   GATE1    = { WRABLE & ADDR1 }
+   GATE2    = { WRABLE & ADDR2 }
+   GATE3    = { WRABLE & ADDR3 }
+   GATE4    = { WRABLE & ADDR4 }
+   GATE5    = { WRABLE & ADDR5 }
+   GATE6    = { WRABLE & ADDR6 }
+   GATE7    = { WRABLE & ADDR7 }
