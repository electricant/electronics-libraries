+   WHEN = { CLEARBAR!='0 & (LOADBAR!='1 ^ CHANGED(LOADBAR,0)) }
+
+ SETUP_HOLD:
+   DATA(1) LOADBAR
+   CLOCK LH = CLOCK
+   SETUPTIME = 240NS
