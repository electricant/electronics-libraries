+   RELEASETIME_HL = 1NS
+ SETUP_HOLD:
+   DATA(1) = PLBAR
+   CLOCK LH = CPU
+   RELEASETIME_LH = 5.7NS
+   WHEN = { MR!='1 }
+ SETUP_HOLD:
+   DATA(1) = PLBAR
+   CLOCK LH = CPD
+   RELEASETIME_LH = 5.7NS
+   WHEN = { MR!='1 }
+ SETUP_HOLD:
+   DATA(4) = P0 P1 P2 P3
+   CLOCK LH = PLBAR
