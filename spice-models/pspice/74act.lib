+ $D_NC   $D_NC   $D_NC   $D_NC   $D_NC   $D_NC   $D_NC   $D_NC
+ D0_EFF IO_ACT 
*
*
U164DLY PINDLY (8,0,4) DPWR DGND
+ Q0 Q1 Q2 Q3 Q4 Q5 Q6 Q7
+ MRBAR CP A B
+ Q0_O Q1_O Q2_O Q3_O Q4_O Q5_O Q6_O Q7_O
+ IO_ACT MNTYMXDLY={MNTYMXDLY} IO_LEVEL={IO_LEVEL}
+ PINDLY:
+   Q0_O Q1_O Q2_O Q3_O Q4_O Q5_O Q6_O Q7_O = {
+     CASE(
+       CHANGED_LH(CP,0), DELAY(1.0NS,9.0NS,12.5NS),
+       CHANGED_HL(MRBAR,0), DELAY(1.0NS,9.5NS,14.5NS),
+       DELAY(2.0NS,10.5NS,15.5NS)  ;DEFAULT
+       )
+     }
+
+ FREQ:
+   NODE = CP
+   MAXFREQ = 80MEG
+
+ WIDTH:
+   NODE = CP
+   MIN_HIGH = 8.0NS
+
+ WIDTH:
+   NODE = MRBAR
+   MIN_LOW =8.0NS
+
+ SETUP_HOLD:
+   CLOCK LH = CP
+   DATA(2) A B
+   SETUPTIME = 8NS
+   HOLDTIME = 1.5NS
+   WHEN = { MRBAR != '0 }
+
+ SETUP_HOLD:
+   CLOCK LH = CP
+   DATA(1) MRBAR
+   RELEASETIME_LH = 2.0NS
*
.ENDS
*
*$
*---------
* 74ACT166  PARALLEL LOAD 8-BIT SHIFT REGISTERS
*
* ADVANCED CMOS LOGIC HD74AC SERIES, HITACHI AMERICA, 1989
* NH   7-22-92      REMODELED USING LOGICEXP, PINDLY, CONSTRAINT DEVICES
*
.SUBCKT 74ACT166 MRBAR_I PEBAR_I CP1_I CP2_I DS_I P0_I P1_I P2_I P3_I P4_I P5_I
+ P6_I P7_I Q7_O
+ OPTIONAL: DPWR=$G_DPWR    DGND=$G_DGND
+ PARAMS:   MNTYMXDLY=0     IO_LEVEL=0
*
UACT166LOG LOGICEXP(20,22) DPWR DGND
+ MRBAR_I PEBAR_I CP1_I CP2_I DS_I P0_I P1_I P2_I P3_I P4_I P5_I P6_I P7_I
+  Q0 Q1 Q2 Q3 Q4 Q5 Q6
+ MRBAR PEBAR CP1 CP2 DS P0 P1 P2 P3 P4 P5 P6 P7 D0 D1 D2 D3 D4 D5 D6 D7 CK
+ D0_GATE   IO_ACT  IO_LEVEL={IO_LEVEL}
+
+ LOGIC:
+   MRBAR  = { MRBAR_I }
+   PEBAR  = { PEBAR_I }
+   CP1    = { CP1_I }
+   CP2    = { CP2_I }
+   DS     = { DS_I }
+   P0     = { P0_I }
+   P1     = { P1_I }
+   P2     = { P2_I }
+   P3     = { P3_I }
+   P4     = { P4_I }
+   P5     = { P5_I }
+   P6     = { P6_I }
+   P7     = { P7_I }
+
* INTERMEDIATE TERM
+   PE = { ~PEBAR }
+
+   D0 = { (PEBAR & DS) | (PE & P0) }
+   D1 = { (PEBAR & Q0) | (PE & P1) }
+   D2 = { (PEBAR & Q1) | (PE & P2) }
+   D3 = { (PEBAR & Q2) | (PE & P3) }
+   D4 = { (PEBAR & Q3) | (PE & P4) }
+   D5 = { (PEBAR & Q4) | (PE & P5) }
+   D6 = { (PEBAR & Q5) | (PE & P6) }
+   D7 = { (PEBAR & Q6) | (PE & P7) }
+   CK = { CP2 | CP1 }
*
U1 DFF(8) DPWR DGND  $D_HI MRBAR CK
+ D0 D1 D2 D3 D4 D5 D6 D7 Q0 Q1 Q2 Q3 Q4 Q5 Q6 Q7
+ $D_NC $D_NC  $D_NC  $D_NC  $D_NC  $D_NC  $D_NC  $D_NC
+ D0_EFF   IO_ACT
*
UACT166DLY PINDLY (1,0,14) DPWR DGND
+ Q7
+ MRBAR CK PEBAR CP1 CP2 DS P0 P1 P2 P3 P4 P5 P6 P7
+ Q7_O
+ IO_ACT
+ MNTYMXDLY={MNTYMXDLY}   IO_LEVEL={IO_LEVEL}
+
+   PINDLY:
+    Q7_O = {
+     CASE(
+       CHANGED_HL(MRBAR,0) & TRN_HL, DELAY(1NS,8.5NS,12NS),
+       CHANGED_LH(CK,0) & TRN_HL, DELAY(1NS,9.5NS,13NS),
+       CHANGED_LH(CK,0) & TRN_LH, DELAY(1NS,10NS,13.5NS),
+       DELAY(1NS,10NS,14NS)  ;DEFAULT
+       )
+     }
+
+ BOOLEAN:
+   NOTCLR_AND_CP2 = { MRBAR!='0 & CP1!='1 }
+   NOTCLR_AND_CP1 = { MRBAR!='0 & CP2!='1 }
+
+ FREQ:
+   NODE = CP1
+   MAXFREQ = 80MEG
+
+ FREQ:
+   NODE = CP2
+   MAXFREQ = 80MEG
+
+ WIDTH:
+   NODE = CP1
+   MIN_HI = 8NS
+   MIN_LO = 8NS
+
+ WIDTH:
+   NODE = CP2
+   MIN_HI = 8NS
+   MIN_LO = 8NS
+
+ WIDTH:
+   NODE = MRBAR
+   MIN_LO = 8NS
+
+ SETUP_HOLD:                              ;CHECK PEBAR
+   DATA(1) PEBAR                          ;CLOCK = CP2
+   CLOCK LH = CP2                         ;CLOCK_INHIBIT = CP1
+   SETUPTIME = 8NS
+   HOLDTIME = 1.5NS
+   WHEN = { NOTCLR_AND_CP2 }
+
+ SETUP_HOLD:                              ;CHECK PEBAR
+   DATA(1) PEBAR                          ;CLOCK = CP1
+   CLOCK LH = CP1                         ;CLOCK_INHIBIT = CP2
+   SETUPTIME = 8NS
+   HOLDTIME = 1.5NS
+   WHEN = { NOTCLR_AND_CP1 }
+
+ SETUP_HOLD:                              ;CHECK PARALLEL DATA
+   DATA(8) P0 P1 P2 P3 P4 P5 P6 P7        ;WHEN IN LOAD MODE
+   CLOCK LH = CP2                         ;CLOCK = CP2
+   SETUPTIME = 8NS                        ;CLOCK_INHIBIT = CP1 = L
+   HOLDTIME = 1.5NS
+   WHEN = { NOTCLR_AND_CP2 & (PEBAR!='1 ^ CHANGED(PEBAR,0)) }
+
+ SETUP_HOLD:                              ;CHECK PARALLEL DATA
+   DATA(8) P0 P1 P2 P3 P4 P5 P6 P7        ;WHEN IN LOAD MODE
+   CLOCK LH = CP1                         ;CLOCK = CP1
+   SETUPTIME = 8NS                        ;CLOCK_INHIBIT = CP2 = L
+   HOLDTIME = 1.5NS
+   WHEN = { NOTCLR_AND_CP1 & (PEBAR!='1 ^ CHANGED(PEBAR,0)) }
+
+ SETUP_HOLD:                              ;CHECK SERIAL DATA
+   DATA(1) DS                             ;WHEN IN SHIFT MODE
+   CLOCK LH = CP2                         ;CLOCK = CP2
+   SETUPTIME = 8NS                        ;CLOCK_INHIBIT = CP1 = L
+   HOLDTIME = 1.5NS
+   WHEN = { NOTCLR_AND_CP2 & (PEBAR!='0 ^ CHANGED(PEBAR,0)) }
+
+ SETUP_HOLD:                              ;CHECK SERIAL DATA
+   DATA(1) DS                             ;WHEN IN SHIFT MODE
+   CLOCK LH = CP1                         ;CLOCK = CP1
+   SETUPTIME = 8NS                        ;CLOCK_INHIBIT = CP2 = L
+   HOLDTIME = 1.5NS
+   WHEN = { NOTCLR_AND_CP1 & (PEBAR!='0 ^ CHANGED(PEBAR,0)) }
+
+ SETUP_HOLD:
+   DATA(1) MRBAR
+   CLOCK LH = CP1
+   RELEASETIME_LH = 0.5NS
+   WHEN = { CP2!='1 }
+
+ SETUP_HOLD:
+   DATA(1) MRBAR
+   CLOCK LH = CP2
+   RELEASETIME_LH = 0.5NS
+   WHEN = { CP1!='1 }
*
.ENDS
