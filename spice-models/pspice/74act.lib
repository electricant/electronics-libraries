+               (DQ1 &  RA &  RB)
+             }
+   Q2      = { (AQ2 & ~RA & ~RB) |
+               (BQ2 &  RA & ~RB) |
+               (CQ2 & ~RA &  RB) |
+               (DQ2 &  RA &  RB)
+             }
+   Q3      = { (AQ3 & ~RA & ~RB) |
+               (BQ3 &  RA & ~RB) |
+               (CQ3 & ~RA &  RB) |
+               (DQ3 &  RA &  RB)
+             }
+   Q4      = { (AQ4 & ~RA & ~RB) |
+               (BQ4 &  RA & ~RB) |
+               (CQ4 & ~RA &  RB) |
+               (DQ4 &  RA &  RB)
+             }
*
UACT670DLY PINDLY (4,1,9) DPWR DGND
+ Q1 Q2 Q3 Q4
+ GRBAR
+ GWBAR RA RB D1 D2 D3 D4 WA WB
+ Q1_O Q2_O Q3_O Q4_O
+ IO_ACT
+ MNTYMXDLY={MNTYMXDLY} IO_LEVEL={IO_LEVEL}
+
+ BOOLEAN:
+   WRITEEN = { CHANGED(GWBAR,0) }
+   READ    = { CHANGED(RA,0) | CHANGED(RB,0) }
+   DATA    = { CHANGED(D1,0) | CHANGED(D2,0) |
+               CHANGED(D3,0) | CHANGED(D4,0) }
+
+ TRISTATE:
+   ENABLE LO GRBAR
+   Q1_O Q2_O Q3_O Q4_O = {
+     CASE (
+                 TRN_ZH, DELAY(1NS, 8.0NS,11.5NS),
+                 TRN_ZL, DELAY(1NS, 9.5NS,12.5NS),
+                 TRN_HZ, DELAY(1NS, 7.0NS,10.5NS),
+                 TRN_LZ, DELAY(1NS, 7.5NS,11.0NS),
+       DATA    & TRN_LH, DELAY(1NS,10.5NS,13.5NS),
+       DATA    & TRN_HL, DELAY(1NS,11.0NS,14.0NS),
+       READ    & TRN_LH, DELAY(1NS,13.0NS,16.5NS),
+       READ    & TRN_HL, DELAY(1NS,11.0NS,14.5NS),
+       WRITEEN         , DELAY(1NS,13.0NS,16.5NS),
+       DELAY(1NS,13.0NS,16.5NS)
+       )
+     }
+
+ WIDTH:
+   NODE      = GWBAR
+   MIN_LO    = 8NS
+ SETUP_HOLD:
+   DATA(4)   = D1 D2 D3 D4
+   CLOCK LH  = GWBAR
+   SETUPTIME = 8NS
+   HOLDTIME  = 4NS
+ SETUP_HOLD:
+   DATA(2)   = WA WB
+   CLOCK HL  = GWBAR
+   SETUPTIME = 8NS
+   HOLDTIME  = .1NS 	   	;WA,WB MUST BE STABLE WHILE GWBAR IS LOW
+ SETUP_HOLD:
+   DATA(2)   = WA WB
+   CLOCK LH  = GWBAR
+   SETUPTIME = .1NS	    	;WA,WB MUST BE STABLE WHILE GWBAR IS LOW
+   HOLDTIME  = 4NS
+ GENERAL:
+   WHEN      = { GWBAR!='1 & (CHANGED(WA,0NS) | CHANGED(WB,0NS)) }
+   MESSAGE   = "WA AND WB MUST BE STABLE WHILE GWBAR IS LOW"
*
