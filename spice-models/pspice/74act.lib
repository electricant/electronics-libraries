+   UPDN = { (CHANGED_LH(CPU,0) | CHANGED_LH(CPD,0)) & PLBAR!='0
+     & MR!='1 }
+   DATA = { (CHANGED(P0,0) | CHANGED(P1,0) | CHANGED(P2,0) | 
+     CHANGED(P3,0)) & PLBAR!='1 & MR!='1 }
+   RESET = { CHANGED_LH(MR,0) }
+ PINDLY:
+   Q0_O Q1_O Q2_O Q3_O = {
