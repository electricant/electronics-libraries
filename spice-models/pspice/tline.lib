+ params: len=1 l1=1 c1=1 r1=0 g1=0 l2=1 c2=1 r2=0 g2=0 lm=1 cm=1

r1 in1 1 {len*r1}
l1 1 2 {len*l1/2}
c1 2 0 {len*(c1)}
l2 2 out1 {len*l1/2}

r3 in2 3 {len*r2}
l3 3 4 {len*l2/2}
c2 4 0 {len*(c2)}
