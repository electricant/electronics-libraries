.param PI2 {3.141592654*2}
.model RG6/U    TRN (r={45.9489u*sqrt(PI2*frq)} l=320.735n
+			g={0.0107663p*PI2*frq}   c=57.0195p)
t A1 A2 B1 B2 rg6/u len={len}
